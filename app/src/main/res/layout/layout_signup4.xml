<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="@dimen/dimen_15dp"
    android:paddingRight="@dimen/dimen_15dp"
    android:paddingTop="@dimen/dimen_20dp">



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen_18dp"
            android:text="学校信息"
            android:textColor="@color/gray33"
            android:textSize="@dimen/sp_20" />

        <include layout="@layout/line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="-25dp"
        android:paddingRight="@dimen/dimen_5dp">

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSchoolProvice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择省份"
            app:title="学校所在地:"></com.zodiac.polit.widget.InputLayout>

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSchoolCity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择城市"
            app:title=""></com.zodiac.polit.widget.InputLayout>

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSchoolArea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择区县"
            app:title=""></com.zodiac.polit.widget.InputLayout>

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSchoolName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择学校"
            app:title=""></com.zodiac.polit.widget.InputLayout>

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSchoolName1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:title="学校名称" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputInOrder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择"
            app:isMust="false"
            app:title="应/往届" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputScience"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择"
            app:isMust="false"
            app:title="文理科" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputMajor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:isMust="false"
            app:title="专业" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputTeamName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:isMust="false"
            app:title="学员队" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputTeamLeaderName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:isMust="false"
            app:title="队干部姓名" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputTeamLeaderPhone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:isMobile="true"
            app:isMust="false"
            app:title="干部手机号" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputEstimateHigh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:title="高考预估" />

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputEstimate1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:editble="false"
            app:eidtHnit="请选择"
            app:title="中考预估" />
    </LinearLayout>

</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:paddingTop="@dimen/dimen_20dp"
    android:paddingLeft="@dimen/dimen_15dp"
    android:paddingRight="@dimen/dimen_15dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_20"
        android:textColor="@color/gray33"
        android:layout_marginBottom="@dimen/dimen_18dp"
        android:text="招飞信息"/>

    <include layout="@layout/line"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="-8dp"
        android:paddingRight="@dimen/dimen_5dp">


        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputSignupYear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:title="报名年份:"
            app:editble="false"
            app:eidtHnit="请选择报名年份"
            ></com.zodiac.polit.widget.InputLayout>


        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputStudentType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:title="学生类型:"
            app:editble="false"
            app:eidtHnit="请选择学生类型"
            ></com.zodiac.polit.widget.InputLayout>

        <com.zodiac.polit.widget.InputLayout
            android:id="@+id/inputProvince"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            app:title="学籍所在省份:"
            app:editble="false"
            app:eidtHnit="请选择招飞省份"></com.zodiac.polit.widget.InputLayout>

    </LinearLayout>

</LinearLayout>